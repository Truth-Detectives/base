    "async def get_video_stats(video_id):\n",
    "    try:\n",
    "        await asyncio.sleep(1)\n",
    "        \n",
    "        response = youtube.videos().list(\n",
    "            part='statistics,snippet',\n",
    "            id=video_id\n",
    "        ).execute()\n",
    "        \n",
    "        print(f\"API Response for {video_id}: {response}\") \n",
    "        \n",
    "        if 'items' not in response or not response['items']:\n",
    "            print(f\"No data was found for this video: {video_id}\")\n",
    "            return {'video_id': video_id, 'views': 0, 'likes': 0, 'comments': 0}\n",
    "\n",
    "        video_info = response['items'][0]\n",
    "        stats = {\n",
    "            'video_id': video_id,\n",
    "            'views': int(video_info['statistics'].get('viewCount', 0)),\n",
    "            'likes': int(video_info['statistics'].get('likeCount', 0)),\n",
    "            'comments': int(video_info['statistics'].get('commentCount', 0)),\n",
    "        }\n",
    "    except Exception as e:\n",
    "        print(f\"Error... Could not fetch stats for {video_id}: {e}\")\n",
    "        stats = {'video_id': video_id, 'views': 0, 'likes': 0, 'comments': 0}\n",
    "    \n",
    "    return stats"
   ]
  },
